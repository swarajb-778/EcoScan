# EcoScan Environment Configuration

# Application Settings
VITE_APP_NAME=EcoScan
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION="AI-Powered Waste Classification"

# API Configuration (if needed for future features)
VITE_API_BASE_URL=https://api.ecoscan.app
VITE_API_VERSION=v1

# Analytics & Monitoring (optional)
VITE_ANALYTICS_ID=
VITE_SENTRY_DSN=
VITE_GTM_ID=

# Feature Flags
VITE_ENABLE_VOICE_INPUT=true
VITE_ENABLE_CAMERA_DETECTION=true
VITE_ENABLE_IMAGE_UPLOAD=true
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG_MODE=false

# Performance Settings
VITE_MODEL_CONFIDENCE_THRESHOLD=0.5
VITE_MODEL_NMS_THRESHOLD=0.4
VITE_MAX_DETECTIONS=20
VITE_INFERENCE_DEVICE=auto

# PWA Settings
VITE_PWA_ENABLED=true
VITE_PWA_THEME_COLOR=#22c55e
VITE_PWA_BACKGROUND_COLOR=#ffffff

# Development Settings
VITE_DEV_ENABLE_MOCK_CAMERA=false
VITE_DEV_ENABLE_MOCK_MODEL=false
VITE_DEV_SHOW_PERFORMANCE_STATS=true

# Production Settings
VITE_PROD_ENABLE_SOURCE_MAPS=false
VITE_PROD_MINIFY=true
VITE_PROD_PRERENDER=true 